{
	"name": "@bluevoid/cli",
	"version": "0.0.0",
	"type": "module",
	"module": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/main.js",
			"types": "./dist/main.d.ts"
		}
	},
	"bin": {
		"bluevoid": "./dist/scripts/cli_entry.js"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"bluevoid": "tsx ./lib/scripts/cli.ts"
	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"fast-glob": "^3.3.3",
		"tsx": "^4.20.6",
		"typescript": "~5.9.3",
		"vite": "npm:rolldown-vite@7.2.5",
		"vite-plugin-dts": "^4.5.4",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"pnpm": {
		"overrides": {
			"vite": "npm:rolldown-vite@7.2.5"
		},
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
